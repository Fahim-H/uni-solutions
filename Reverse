//Write a function with the following header to display an integer in reverse order:
#include <iostream>
#include <iomanip>
#include <stdio.h>
using namespace std;
void reverse(int n)
{	while(n!=0)
	{	cout<<n%10;
		n=n/10;
	}	cout<<' ';
}
int main()
{
    while(true){
        int in; cin>>in;
        if(in==0)break;
        reverse(in);
    }
    return 0;
}
